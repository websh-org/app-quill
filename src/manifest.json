{
  "v": 0,
  "name": "Quill",
  "icon": "icon.svg",
  "description": "Quill rich text editor for WebShell",
  "version": "1.3.7",
  "license": "MIT",
  "homepage": "https://github.com/websh-org/app-quill#readme",
  "repository": "https://github.com/websh-org/app-quill.git",
  "api": {
    "file": {
      "new": "quill",
      "open": ["quill","text"],
      "save": ["quill","text","html"],
      "formats": {
        "quill": {
          "name": "Quill File",
          "type": "application/json",
          "extension": "quill",
          "encoding": "text",
        },
        "html": {
          "name": "HTML File",
          "type": "text/html",
          "extension": "html",
          "encoding": "text",
        },
        "text": {
          "name": "Text File",
          "type": "text/plain",
          "encoding": "text",
          "extension": "txt",
        }
      }
    }
  }
}